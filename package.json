{
    "name": "witai-client",
    "version": "0.1.0",
    "description": "A client for [wit.ai](https://wit.ai) apps.",
    "keywords": [],
    "homepage": "https://github.com/rikhoffbauer/witai-client#readme",
    "bugs": {
        "url": "https://github.com/rikhoffbauer/witai-client/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rikhoffbauer/witai-client.git"
    },
    "license": "MIT",
    "author": "Rik Hoffbauer <git@rikhoffbauer.com> (https://github.com/rikhoffbauer)",
    "type": "module",
    "exports": {
        "node": "./dist/index.cjs",
        "require": "./dist/index.cjs",
        "script": "./dist/index.umd.js",
        "module": "./dist/index.module.js",
        "default": "./dist/index.modern.js"
    },
    "main": "dist/index.cjs",
    "umd:main": "./dist/index.umd.js",
    "unpkg": "./dist/index.umd.js",
    "module": "./dist/index.module.js",
    "source": "./src/index.ts",
    "typings": "./dist/index.d.ts",
    "scripts": {
        "fmt": "tools/fmt",
        "test": "jest --coverage",
        "prebuild": "rimraf dist",
        "build": "microbundle",
        "prepublishOnly": "pnpm build",
        "build:watch": "microbundle watch",
        "start": "pnpm run start:prod",
        "start:prod": "node dist/main",
        "start:dev": "tsc-watch --onSuccess \"node dist/main.js\"",
        "start:debug": "tsc-watch --onSuccess \"node --inspect-brk dist/main.js\""
    },
    "husky": {
        "skipCI": false,
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,md,json,yml,yaml,bash,sh,zsh,xml,iml,html,css,scss}": [
            "prettier --loglevel warn --write"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "prettier": {
        "arrowParens": "avoid",
        "endOfLine": "lf",
        "plugins": [
            "prettier-plugin-sh",
            "@prettier/plugin-xml",
            "prettier-plugin-import-sort"
        ],
        "printWidth": 80,
        "tabWidth": 4,
        "trailingComma": "all",
        "overrides": [
            {
                "files": "*.md",
                "options": {
                    "parser": "markdown",
                    "printWidth": 70,
                    "proseWrap": "never",
                    "trailingComma": "none",
                    "useTabs": false
                }
            },
            {
                "files": [
                    "*.json"
                ],
                "options": {
                    "parser": "json",
                    "quoteProps": "consistent",
                    "singleQuote": false,
                    "tabWidth": 4,
                    "trailingComma": "none"
                }
            },
            {
                "files": [
                    "*.bash",
                    "*.sh",
                    "*.zsh",
                    "tools/*"
                ],
                "options": {
                    "parser": "sh"
                }
            },
            {
                "files": [
                    "*.xml",
                    "*.iml"
                ],
                "options": {
                    "parser": "xml",
                    "printWidth": 80,
                    "tabWidth": 4,
                    "xmlWhitespaceSensitivity": "strict"
                }
            }
        ]
    },
    "jest": {
        "bail": true,
        "cache": false,
        "clearMocks": true,
        "collectCoverage": true,
        "coverageDirectory": "<rootDir>/coverage",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/__test__/.*\\.(ts|js)$",
            "/build/.*\\.(ts|js)$"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ],
        "moduleDirectories": [
            "node_modules",
            "<rootDir>/src"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "modulePathIgnorePatterns": [
            "/build/"
        ],
        "preset": "ts-jest",
        "setupFiles": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
            "/build/"
        ],
        "testRegex": "(.*\\.(spec))\\.(ts|tsx|js)$",
        "verbose": true
    },
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@prettier/plugin-xml": "^1.0.2",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/commit-analyzer": "^8.0.1",
        "@semantic-release/git": "^9.0.0",
        "@semantic-release/github": "^7.2.3",
        "@semantic-release/npm": "^7.1.3",
        "@semantic-release/release-notes-generator": "^9.0.3",
        "@types/jest": "^26.0.24",
        "@types/moxios": "^0.4.12",
        "@types/node": "^16.4.13",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "@typescript-eslint/parser": "^4.29.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^7.0.1",
        "import-sort": "^6.0.0",
        "import-sort-parser-typescript": "^6.0.0",
        "import-sort-style-custom": "^2.1.2",
        "import-sort-style-module": "^6.0.0",
        "jest": "^27.0.6",
        "jest-cli": "^27.0.6",
        "lint-staged": "^11.1.2",
        "microbundle": "^0.13.3",
        "moxios": "^0.4.0",
        "prettier": "^2.3.2",
        "prettier-plugin-import-sort": "^0.0.7",
        "prettier-plugin-sh": "^0.7.1",
        "rimraf": "^3.0.2",
        "semantic-release": "github:rikhoffbauer/semantic-release",
        "sort-package-json": "^1.50.0",
        "ts-jest": "^27.0.4",
        "ts-node": "^10.1.0",
        "tsc-watch": "^4.4.0",
        "typescript": "^4.3.5"
    },
    "importSort": {
        ".js, .jsx, .es6, .es, .mjs": {
            "parser": "babylon",
            "style": "import-sort-style-custom"
        },
        ".ts, .tsx": {
            "parser": "typescript",
            "style": "import-sort-style-custom"
        }
    },
    "release": {
        "branches": [
            {
                "name": "main",
                "prerelease": false
            },
            {
                "name": "next",
                "prerelease": "beta",
                "channel": "next"
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            [
                "@semantic-release/github"
            ],
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "package.json",
                        "CHANGELOG.md"
                    ],
                    "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
                }
            ]
        ]
    },
    "wallaby": {
        "autoDetect": true,
        "runMode": "onsave"
    }
}
